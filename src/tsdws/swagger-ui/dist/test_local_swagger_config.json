                    "timeseries"
                ],
                "summary": "Register a timeseries",
                "description": "",
                "operationId": "timeseries-post",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [{
                    "in": "body",
                    "name": "body",
                    "description": "JSON object that needs to be passed as input to register a timeseries into the database",
                    "required": true,
                    "schema": {
                        "$ref": "#/definitions/Timeseries"
                    }
                }],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    },
                    "207": {
