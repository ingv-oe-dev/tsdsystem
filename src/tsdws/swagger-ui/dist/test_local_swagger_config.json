                    "409": {
                        "description": "Operation failed"
                    }
                },
                "security": [{
                    "Bearer": []
                }]
            }
        },
        "/token": {
            "post": {
                "tags": [
                    "token"
                ],
                "summary": "Retrieve an authorization token (JWT) using user credentials",
                "description": "",
                "operationId": "token",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [{
                        "name": "email",
                        "in": "formData",
                        "description": "Username (email)",
                        "type": "string",
                        "required": true
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "Password",
                        "type": "string",
                        "required": true
                    },
                    {
                        "name": "scope",
                        "in": "formData",
                        "description": "Scope of the authorization token",
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "admin",
                                "all",
                                "channels",
                                "channels-edit",
                                "channels-read",
                                "nets",
                                "nets-edit",
                                "nets-read",
                                "owners",
                                "owners-edit",
                                "owners-read",
                                "sensors",
                                "sensors-edit",
                                "sensors-read",
                                "sensortypes",
                                "sensortypes-edit",
                                "sensortypes-read",
                                "sites",
                                "sites-edit",
                                "sites-read",
                                "timeseries",
                                "timeseries-edit",
                                "timeseries-read"
                            ],
                            "default": ""
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "statusCode": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "error": {
                                    "type": "string"
                                },
                                "token": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
