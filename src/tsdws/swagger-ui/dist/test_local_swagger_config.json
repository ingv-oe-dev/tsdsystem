                            "$ref": "#/definitions/ApiResponse"
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "401": {
                        "description": "Authorization not found"
                    },
                    "409": {
                        "description": "Operation failed"
                    }
                },
                "security": [{
                    "Bearer": []
                }]
            },
            "patch": {
                "tags": [
                    "timeseries"
                ],
                "summary": "Update timeseries",
                "description": "",
                "operationId": "timeseries-patch",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [{
                    "in": "body",
                    "name": "body",
